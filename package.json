{
	"name": "genius-dark",
	"version": "2.0.0",
	"description": "Dark mode theme for Genius",
	"private": true,
	"scripts": {
		"lint": "stylelint src",
		"lint:fix": "stylelint src --fix",
		"style": "prettier src -w && npm run lint:fix",
		"build": "ts-node scripts/build.ts && prettier genius-dark.user.css -w",
		"watch": "sass --no-source-map --watch src/:dist/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vBread/Genius-Dark.git"
	},
	"keywords": [],
	"author": "Bread",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/vBread/Genius-Dark/issues"
	},
	"homepage": "https://github.com/vBread/Genius-Dark#readme",
	"devDependencies": {
		"@types/node": "^14.14.21",
		"prettier": "^2.2.1",
		"stylelint": "^13.9.0",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-order": "^4.1.0",
		"stylelint-scss": "^3.18.0"
	}
}
